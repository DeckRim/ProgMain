#include <iostream>
#include <string>
#include <map>

using namespace std;

int main() {
    map<double, string> вещества = {
        { 252, "Гелий" },
        { 246, "Неон" },
        { 185, "Азот" },
        { 182, "Фтор" },
        { 153, "Анилин" },
        { 101, "Спирт" },
        { 88, "Серная кислота" },
        { 71, "Соляная кислота" },
        { 61, "Бензол" },
        { 34, "Бензин" }
    
 };

    double температураКипения;

    cout << "Введите температуру кипения вещества: ";
    cin >> температураКипения;

    for (auto it = вещества.begin(); it != вещества.end(); ++it) {
        if (it->first == температураКипения) {
            cout << "Вещество: " << it->second << endl;
            return 0;
        }
    }

    cout << "Вещество с такой температурой кипения не найдено." << endl;

    return 0;
}
